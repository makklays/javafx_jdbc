<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.chart.AreaChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.chart.CategoryAxis?>

<AnchorPane xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.techmatrix.javafx_jdbc.view.Statistics2Controller"
            prefHeight="600.0" prefWidth="850.0">
    <children>
        <Label alignment="TOP_LEFT" layoutX="24.0" layoutY="19.0" prefHeight="21.0" prefWidth="484.0"
               text="Statistics" style="-fx-font-size: 20px; -fx-font-weight: bold;" />
        <Label layoutX="24.0" layoutY="45.0" prefHeight="16.0" prefWidth="473.0" text="Statistics of data" />

        <AreaChart fx:id="StatAreaChart" layoutX="24.0" layoutY="100.0" prefWidth="600" prefHeight="400">
            <xAxis>
                <CategoryAxis fx:id="xAxis"/>
            </xAxis>
            <yAxis>
                <NumberAxis fx:id="yAxis"/>
            </yAxis>
        </AreaChart>

    </children>
</AnchorPane>

